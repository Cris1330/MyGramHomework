            <!-- Button trigger modal -->
            <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#Pictures">
                Launch demo modal
            </button>
            <!-- Modal -->
            <div class="modal fade" id="Pictures" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="rainbow" id="exampleModalLabel">Rainbow</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            
                            <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
                                <div class="carousel-inner">

                                    <div class="carousel-item active">
                                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTY8p6Ld3juBggVypT-fjHihzCdQVdjNRrZXg&usqp=CAU" class="d-block w-100" />
                                    </div>

                                    <div class="carousel-item">
                                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRtonzPTYxIn4945DI0NOrFjZtLmj0Ej9SrsA&usqp=CAU" class="d-block w-100" />

                                    </div>
                                    <div class="carousel-item">
                                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQwmWiIUEOLX0tJzSpYP-Ule11A_G40BJvt5A&usqp=CAU" class="d-block w-100" />
                                    </div>
                                    <div class="carousel-item">
                                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSJesdM1lvB5C4IXbzyTw6vm_Alwt0mPRy81A&usqp=CAU" class="d-block w-100" />
                                    </div>

                                </div>

                                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExamControl"
                                        data-bs-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hiden="true"></span>
                                    <span class="visual-hidden">Previous</span>
                                </button>

                                <button class="carousel-control-next" type="button" data-bs-target="#carouselExamControl"
                                        data-bs-slide="next">
                                    <span class="carousel-control-next-icon" aria-hiden="true"></span>
                                    <span class="visual-hidden">Next</span>
                                </button>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary">Save changes</button>
                        </div>
                    </div>
                </div>
            </div>





            <div class="col-4"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTYznsU3Tzja_JJ4P94FNMQRskjxrwOOPPNFQ&usqp=CAU" /></div>

